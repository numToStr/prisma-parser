datasource db {
    provider = "mysql"
    url      = env("DATABASE_URL")
    hello = ["hello", "hello"]
}

generator client {
    provider = "prisma-client-js"
}

enum Role {
  OPERATIVE
  BACKOFFICE
  ADMIN
}

model User {
    id          Int         @id
    username    String?     @unique @updatedAt
    fullname    String?     @db.Varchar(123)
    role        Role        @default(OPERATIVE, 123)

    @@id()
}

